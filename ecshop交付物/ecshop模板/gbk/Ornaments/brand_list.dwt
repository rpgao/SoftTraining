<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gbk" />
<meta name="Keywords" content="{$keywords}" />
<meta name="Description" content="{$description}" />
<!-- TemplateBeginEditable name="doctitle" -->
<title>{$page_title}</title>
<!-- TemplateEndEditable --><!-- TemplateBeginEditable name="head" --><!-- TemplateEndEditable -->
<link rel="shortcut icon" href="favicon.ico" />
<link rel="icon" href="animated_favicon.gif" type="image/gif" />
<link href="{$ecs_css_path}" rel="stylesheet" type="text/css" />
{* 包含脚本文件 *}
{insert_scripts files='common.js,global.js,compare.js'}
</head>
<body>
<!-- #BeginLibraryItem "/library/page_header.lbi" --><!-- #EndLibraryItem -->
<!-- #BeginLibraryItem "/library/search_form.lbi" --><!-- #EndLibraryItem -->
<!-- #BeginLibraryItem "/library/ur_here.lbi" --><!-- #EndLibraryItem -->
<!--主体内容 start-->
<div class="block">
	<div class="mod_title3">
		<div class="boxcontent">
			<div class="left fl">{$lang.all_brand}</div>
			<div class="right fr">&nbsp;</div>
		</div>
	</div>
	<div class="goods_filter brands_list">
	 <!-- {foreach from=$brand_list name="brand_list_foreach" item=brand_data} -->
      <div class="goodsbox">
      <h4 class="clearfix"><span class="fl"><a href="{$brand_data.url}">{$brand_data.brand_name}</a></span><span class="fr">({$brand_data.goods_num})</span></h4>
      <!-- {if $brand_data.brand_logo} -->
       <div class="imgbox">
			 <a href="{$brand_data.url}"><img src="data/brandlogo/{$brand_data.brand_logo}" alt="{$brand_data.brand_name|escape:html} ({$brand_data.goods_num})" /></a>
			 </div>
      <!-- {/if} -->
       <p title="{$brand_data.brand_desc}" class="word tl f1">{$brand_data.brand_desc}</p> 
      </div>
    <!-- {/foreach} -->
  </div>
</div>
<!--主体内容 end-->
<!-- #BeginLibraryItem "/library/help.lbi" --><!-- #EndLibraryItem -->
<!-- #BeginLibraryItem "/library/page_footer.lbi" --><!-- #EndLibraryItem -->
</body>
</html>
